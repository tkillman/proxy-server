{
  "version": 2,
  "regions": ["icn1"],
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "headers": [
    { "source": "/(.*)"
      ,"headers": [
        {
          "key": "version",
          "value": "staging"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://top-shop.logiall.com"
    }
  ]
}
